      ~/.config/rofi/applets/menu/network.sh

super + u
      ~/.config/rofi/applets/menu/time.sh

super + z
      ~/.config/rofi/applets/menu/volume.sh

super + t
      ~/.config/rofi/applets/menu/mpd.sh

super + p
      dmenu_run -x 6 -y 6 -z 1354 -h 20

super + a
      feh --bg-scale ~/Documents/Pictures/wallpaper/ --randomize --no-fehbg

super + o
      bash dmenu.sh

super + e
      emacs

super + shift + e
      nautilus

mod1+ shift + e
      st -e ranger

super + shift + d
      discocss

super + shift + s
      flameshot gui

